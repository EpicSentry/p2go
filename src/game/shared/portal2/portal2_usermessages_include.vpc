$MacroRequired	PROTOBUF_BUILDER_INCLUDED

$Project
{
	$Folder "Protobuf Files"
	{
		$File "$SRCDIR\game\shared\portal2\portal2_usermessages.proto"
		$Folder "Generated Files"
		{
			$DynamicFile "$GENERATED_PROTO_DIR\portal2_usermessages.pb.h"
			$DynamicFile "$GENERATED_PROTO_DIR\portal2_usermessages.pb.cc"
			{
				$Configuration
				{
					$Compiler [$WINDOWS]
					{
						$Create/UsePrecompiledHeader	"Not Using Precompiled Headers"
					}
				}
			}
		}
	}

}
